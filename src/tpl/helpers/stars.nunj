{% from '../helpers/svg.nunj' import svg %}

{% macro rating(rating) %}
    {% for item in range(1, 6) %}
        {% if item <= rating %}
            {{ svg('icon-star', 'rating--active star-icon') }}
        {% else %}
            {{ svg('icon-star', 'rating--in-active star-icon') }}
        {% endif %}
    {% endfor %}
{% endmacro %}
